<div id="<%= dom_id pet %>">
  <p>
    <strong>Name:</strong>
    <%= pet.name %>
  </p>

  <p>
    <strong>Type:</strong>
    <%= pet.type %>
  </p>

  <p>
    <strong>Size:</strong>
    <%= pet.size %>
  </p>

  <p>
    <strong>Gender:</strong>
    <%= pet.gender %>
  </p>

  <p>
    <strong>Id user:</strong>
    <%= pet.id_user %>
  </p>

  <p>
    <strong>Id city:</strong>
    <%= pet.id_city %>
  </p>

</div>
